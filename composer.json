{
  "name": "certado/sendinblue-api-v3-sdk",
  "description": "SendinBlue provided RESTFul API V3 php library",
  "keywords": [
    "sendinblue",
    "swagger",
    "php",
    "sdk",
    "api"
  ],
  "homepage": "https://github.com/certado/APIv3-php-library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7.5.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.25",
    "squizlabs/php_codesniffer": "~3.7.1",
    "friendsofphp/php-cs-fixer": "~v2.19.3"
  },
  "autoload": {
    "psr-4": {
      "SendinBlue\\Client\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SendinBlue\\Client\\": "test/"
    }
  }
}
